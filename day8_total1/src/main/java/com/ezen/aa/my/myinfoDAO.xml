<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ezen.aa.my.MyinfoService">

<insert id="myinfoinsert" parameterType="com.ezen.aa.my.MyinfoDTO">
insert into myinfo0914 values(#{name},#{gender},#{age})
</insert>

<select id="myinfoout" resultType="com.ezen.aa.my.MyinfoDTO">
select * from myinfo0914
</select>

<select id="myinfoout2" resultType="com.ezen.aa.my.MyinfoDTO">
select * from myinfo0914 where age>=30
</select>

</mapper>